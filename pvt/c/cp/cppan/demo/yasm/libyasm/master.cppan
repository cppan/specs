    "cppan": "source:\n    git: https:\/\/github.com\/yasm\/yasm\n    branch: master\n\nversion: master\n\ntype: library\n\napi_name:\n    -   YASM_LIB_DECL\n\nfiles:\n    -   libyasm-stdint.h.cmake\n    -   libyasm.h\n    -   libyasm\/[^\/]*\n    -   util.h\n\nexclude_from_package:\n    -   .*\/tests\/.*\n\nexclude_from_build:\n    -   .*\/tests\/.*\n    -   libyasm\/cmake-module.c\n\ndependencies:\n    public:\n        pvt.cppan.demo.yasm.modules.dbgfmts.stabs:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.objfmts.xdf:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.dbgfmts.codeview:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.dbgfmts.null:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.objfmts.bin:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.objfmts.coff:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.preprocs.nasm:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.preprocs.gas:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.arch.lc3b:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.objfmts.dbg:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.objfmts.macho:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.parsers.nasm:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.preprocs.cpp:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.parsers.gas:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.objfmts.elf:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.dbgfmts.dwarf2:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.preprocs.raw:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.objfmts.rdf:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.arch.x86:\n            version: \"*\"\n        pvt.cppan.demo.yasm.modules.listfmts.nasm:\n            version: \"*\"\n\npost_sources: |\n    configure_file(${SDIR}\/libyasm-stdint.h.cmake ${BDIR}\/libyasm-stdint.h)\n    \n    \n    replace_in_file_once(${SDIR}\/libyasm\/module.in \"EXTERN_LIST\" \"\n        #define ADD_MODULE(x, t) {#x, &yasm_ ## x ## _LTX_ ## t,},\n    \n        extern yasm_arch_module yasm_lc3b_LTX_arch;\n        extern yasm_arch_module yasm_x86_LTX_arch;\n    \n        extern yasm_dbgfmt_module yasm_stabs_LTX_dbgfmt;\n        extern yasm_dbgfmt_module yasm_dwarf2_LTX_dbgfmt;\n        extern yasm_dbgfmt_module yasm_null_LTX_dbgfmt;\n        extern yasm_dbgfmt_module yasm_cv8_LTX_dbgfmt;\n    \n        extern yasm_listfmt_module yasm_nasm_LTX_listfmt;\n    \n        #\n        extern yasm_objfmt_module yasm_dosexe_LTX_objfmt;\n        extern yasm_objfmt_module yasm_bin_LTX_objfmt;\n    \n        extern yasm_objfmt_module yasm_coff_LTX_objfmt;\n        extern yasm_objfmt_module yasm_win32_LTX_objfmt;\n        extern yasm_objfmt_module yasm_win64_LTX_objfmt;\n        extern yasm_objfmt_module yasm_x64_LTX_objfmt;\n    \n        extern yasm_objfmt_module yasm_dbg_LTX_objfmt;\n    \n        extern yasm_objfmt_module yasm_elf_LTX_objfmt;\n        extern yasm_objfmt_module yasm_elf32_LTX_objfmt;\n        extern yasm_objfmt_module yasm_elf64_LTX_objfmt;\n        extern yasm_objfmt_module yasm_elfx32_LTX_objfmt;\n    \n        extern yasm_objfmt_module yasm_macho_LTX_objfmt;\n        extern yasm_objfmt_module yasm_macho32_LTX_objfmt;\n        extern yasm_objfmt_module yasm_macho64_LTX_objfmt;\n    \n        extern yasm_objfmt_module yasm_rdf_LTX_objfmt;\n    \n        extern yasm_objfmt_module yasm_xdf_LTX_objfmt;\n    \n        #\n        extern yasm_parser_module yasm_gas_LTX_parser;\n        extern yasm_parser_module yasm_gnu_LTX_parser;\n        extern yasm_parser_module yasm_nasm_LTX_parser;\n        extern yasm_parser_module yasm_tasm_LTX_parser;\n    \n        extern yasm_preproc_module yasm_cpp_LTX_preproc;\n        extern yasm_preproc_module yasm_gas_LTX_preproc;\n        extern yasm_preproc_module yasm_tasm_LTX_preproc;\n        extern yasm_preproc_module yasm_nasm_LTX_preproc;\n        extern yasm_preproc_module yasm_raw_LTX_preproc;\n    \")\n    replace_in_file_once(${SDIR}\/libyasm\/module.in \"MODULES_arch_\" \"\n        ADD_MODULE(lc3b, arch)\n        ADD_MODULE(x86, arch)\n    \")\n    replace_in_file_once(${SDIR}\/libyasm\/module.in \"MODULES_dbgfmt_\" \"\n        ADD_MODULE(stabs, dbgfmt)\n        ADD_MODULE(dwarf2, dbgfmt)\n        ADD_MODULE(null, dbgfmt)\n        ADD_MODULE(cv8, dbgfmt)\n    \")\n    replace_in_file_once(${SDIR}\/libyasm\/module.in \"MODULES_objfmt_\" \"\n        ADD_MODULE(dosexe, objfmt)\n        ADD_MODULE(bin, objfmt)\n    \n        ADD_MODULE(coff, objfmt)\n        ADD_MODULE(win32, objfmt)\n        ADD_MODULE(win64, objfmt)\n        ADD_MODULE(x64, objfmt)\n    \n        ADD_MODULE(dbg, objfmt)\n    \n        ADD_MODULE(elf, objfmt)\n        ADD_MODULE(elf32, objfmt)\n        ADD_MODULE(elf64, objfmt)\n        ADD_MODULE(elfx32, objfmt)\n    \n        ADD_MODULE(macho, objfmt)\n        ADD_MODULE(macho32, objfmt)\n        ADD_MODULE(macho64, objfmt)\n    \n        ADD_MODULE(rdf, objfmt)\n    \n        ADD_MODULE(xdf, objfmt)\n    \")\n    replace_in_file_once(${SDIR}\/libyasm\/module.in \"MODULES_listfmt_\" \"\n        ADD_MODULE(nasm, listfmt)\n    \")\n    replace_in_file_once(${SDIR}\/libyasm\/module.in \"MODULES_parser_\" \"\n        ADD_MODULE(gas, parser)\n        ADD_MODULE(gnu, parser)\n        ADD_MODULE(nasm, parser)\n        ADD_MODULE(tasm, parser)\n    \")\n    replace_in_file_once(${SDIR}\/libyasm\/module.in \"MODULES_preproc_\" \"\n        ADD_MODULE(cpp, preproc)\n        ADD_MODULE(gas, preproc)\n        ADD_MODULE(tasm, preproc)\n        ADD_MODULE(nasm, preproc)\n        ADD_MODULE(raw, preproc)\n    \")\n    configure_file(${SDIR}\/libyasm\/module.in ${BDIR}\/module.c)\n    set(src ${src} ${BDIR}\/module.c)\n\ncheck_function_exists:\n    -   mergesort\n    -   popen\n    -   strcasecmp\n    -   strcmpi\n    -   stricmp\n    -   strsep\n    -   _popen\n    -   _stricmp\n\ncheck_include_exists:\n    -   file: libgen.h\n        variable: HAVE_LIBGEN_H\n        cpp: false\n    -   file: locale.h\n        variable: HAVE_LOCALE_H\n        cpp: false\n    -   file: strings.h\n        variable: HAVE_STRINGS_H\n        cpp: false\n\n",
