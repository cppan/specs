SDIR}\/cmake\/cl2cpp.cmake\"\n          DEPENDS ${cl_kernels} \"${SDIR}\/cmake\/cl2cpp.cmake\")\n        list(APPEND src \"${BDIR}\/${OCL_NAME}.cpp\" \"${BDIR}\/${OCL_NAME}.hpp\")\n    endif()\n    \n    set(OPENCV_CPU_BASELINE_DEFINITIONS_CONFIGMAKE \"\n        #define CV_CPU_COMPILE_SSE 1\n        #define CV_CPU_BASELINE_COMPILE_SSE 1\n    \n        #define CV_CPU_COMPILE_SSE2 1\n        #define CV_CPU_BASELINE_COMPILE_SSE2 1\n    \n        #define CV_CPU_BASELINE_FEATURES 0 \\\n            , CV_CPU_SSE \\\n            , CV_CPU_SSE2 \\\n    \n            \n    \")\n    \n    set(OPENCV_CPU_DISPATCH_DEFINITIONS_CONFIGMAKE \"\n        #define CV_CPU_DISPATCH_COMPILE_SSE4_1 1\n        #define CV_CPU_DISPATCH_COMPILE_SSE4_2 1\n        #define CV_CPU_DISPATCH_COMPILE_FP16 1\n    \")\n    \n    macro(add_dispatch_file fn)\n        file_write_once(${BDIR}\/${fn}.simd_declarations.hpp \"\n            #define CV_CPU_SIMD_FILENAME \\\"${fn}.simd.hpp\\\"\n            #define CV_CPU_DISPATCH_MODE SSE2\n            #include \\\"opencv2\/core\/private\/cv_cpu_include_simd_declarations.hpp\\\"\n    \n            #define CV_CPU_DISPATCH_MODES_ALL SSE2, BASELINE\n        \")\n    endmacro()\n    add_dispatch_file(mathfuncs_core)\n    add_dispatch_file(stat)\n    \n    file_write_once(${BDIR}\/precomp.hpp \"#include <opencv2\/core\/private.hpp>\")\n    file_write_once(${BDIR}\/opencv2\/opencv_modules.hpp \"\")\n    file_write_once(${BDIR}\/version_string.inc \"\\\"OpenCV - ${PACKAGE_NAME} - ${PACKAGE_VERSION}\\\\n\\\"\\n\\\"Powered by C++ Archive Network\\\"\")\n    configure_file(${SDIR}\/cmake\/templates\/cvconfig.h.in ${BDIR}\/cvconfig.h)\n    configure_file(${SDIR}\/cmake\/templates\/cv_cpu_config.h.in ${BDIR}\/cv_cpu_config.h)\n    configure_file(${SDIR}\/cmake\/templates\/custom_hal.hpp.in ${BDIR}\/custom_hal.hpp)\n\n",
