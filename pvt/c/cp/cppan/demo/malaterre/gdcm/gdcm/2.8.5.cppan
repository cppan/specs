{
    "project": "pvt.cppan.demo.malaterre.gdcm.gdcm",
    "cppan": "source:\n    git: https:\/\/github.com\/malaterre\/GDCM\n    tag: v{v}\n\nversion: 2.8.5\n\ntype: library\n\nroot_directory: Source\n\nfiles:\n    -   .*\\.cpp\n    -   .*\\.cxx\n    -   .*\\.h\n    -   .*\\.hpp\n    -   .*\\.txx\n    -   Common\/gdcmConfigure.h.in\n\nexclude_from_package:\n    -   Common\/gdcmTesting.cxx\n    -   DataDictionary\/gdcmPrepDict.cxx\n    -   DataDictionary\/gdcmPrepGroupName.cxx\n    -   DataDictionary\/gdcmRoot.cxx\n    -   DataStructureAndEncodingDefinition\/gdcmParser.cxx\n    -   InformationObjectDefinition\/gdcmXMLDictReader.cxx\n    -   InformationObjectDefinition\/gdcmXMLPrivateDictReader.cxx\n    -   MediaStorageAndFileFormat\/gdcmDeltaEncodingCodec.cxx\n\nexclude_from_build:\n    -   MediaStorageAndFileFormat\/gdcmJPEGBITSCodec.cxx\n\ninclude_directories:\n    public:\n        -   DataStructureAndEncodingDefinition\n        -   InformationObjectDefinition\n        -   Common\n        -   MessageExchangeDefinition\n        -   DataDictionary\n        -   MediaStorageAndFileFormat\n\noptions:\n    any:\n        definitions:\n            public:\n                -   GDCM_USE_OPENJPEG_V2\n                -   GDCM_USE_SYSTEM_CHARLS\n                -   GDCM_USE_SYSTEM_EXPAT\n                -   GDCM_USE_SYSTEM_JSON\n                -   GDCM_USE_SYSTEM_OPENSSL\n        link_libraries:\n            win32:\n                public:\n                    -   Rpcrt4\n    shared:\n        definitions:\n            private:\n                -   gdcmCommon_EXPORTS\n            public:\n                -   GDCM_BUILD_SHARED_LIBS\n\ndependencies:\n    public:\n        pvt.cppan.demo.openssl.crypto:\n            version: 1\n        pvt.cppan.demo.madler.zlib:\n            version: 1\n        pvt.cppan.demo.openjpeg.openjp2:\n            version: 2\n        pvt.cppan.demo.json_c:\n            version: master\n        pvt.cppan.demo.expat:\n            version: 2\n        pvt.cppan.demo.vbaderks.charls:\n            version: 1\n    private:\n        pvt.cppan.demo.malaterre.gdcm.socketxx:\n            version: \"*\"\n        pvt.cppan.demo.malaterre.gdcm.jpeg8:\n            version: \"*\"\n        pvt.cppan.demo.malaterre.gdcm.jpeg16:\n            version: \"*\"\n        pvt.cppan.demo.malaterre.gdcm.jpeg12:\n            version: \"*\"\n        pvt.cppan.demo.malaterre.gdcm.rle:\n            version: \"*\"\n\npost_sources: |\n    set(GDCM_MAJOR_VERSION ${PACKAGE_MAJOR_VERSION})\n    set(GDCM_MINOR_VERSION ${PACKAGE_MINOR_VERSION})\n    set(GDCM_BUILD_VERSION ${PACKAGE_PATCH_VERSION})\n    set(GDCM_VERSION ${PACKAGE_VERSION})\n    \n    macro(setv v)\n        set(GDCM_${v} ${${v}})\n    endmacro(setv)\n    \n    setv(HAVE_SYS_TIME_H)\n    setv(HAVE_WINSOCK_H)\n    setv(HAVE_BYTESWAP_H)\n    setv(HAVE_RPC_H)\n    setv(HAVE_STRCASECMP)\n    setv(HAVE_STRNCASECMP)\n    \n    setv(HAVE_SNPRINTF)\n    setv(HAVE__SNPRINTF)\n    setv(HAVE_STRPTIME)\n    setv(HAVE__STRICMP)\n    setv(HAVE__STRNICMP)\n    setv(HAVE__SNPRINTF)\n    \n    configure_file(\"${SDIR}\/Common\/gdcmConfigure.h.in\" \"${BDIR}\/gdcmConfigure.h\")\n    replace_in_file_once(${SDIR}\/Common\/zipstreamimpl.h \"gdcm_zlib.h\" \"zlib.h\")\n    replace_in_file_once(${SDIR}\/InformationObjectDefinition\/gdcmTableReader.cxx \"gdcm_expat.h\" \"expat.h\")\n    replace_in_file_once(${SDIR}\/MediaStorageAndFileFormat\/gdcmJPEG2000Codec.cxx \"gdcm_openjpeg.h\" \"openjpeg.h\")\n    replace_in_file_once(${SDIR}\/MediaStorageAndFileFormat\/gdcmJPEG2000Codec.cxx \"gdcm_openjpeg2.h\" \"openjpeg.h\")\n    \n    replace_in_file_once(${SDIR}\/MediaStorageAndFileFormat\/gdcmJPEG8Codec.cxx \"gdcm_ljpeg8.h\"\n        \"8\/jinclude.h\\\"\\n#include \\\"8\/jpeglib.h\\\"\\n#include \\\"8\/jerror.h\")\n    replace_in_file_once(${SDIR}\/MediaStorageAndFileFormat\/gdcmJPEG12Codec.cxx \"gdcm_ljpeg12.h\"\n        \"12\/jinclude.h\\\"\\n#include \\\"12\/jpeglib.h\\\"\\n#include \\\"12\/jerror.h\")\n    replace_in_file_once(${SDIR}\/MediaStorageAndFileFormat\/gdcmJPEG16Codec.cxx \"gdcm_ljpeg16.h\"\n        \"16\/jinclude.h\\\"\\n#include \\\"16\/jpeglib.h\\\"\\n#include \\\"16\/jerror.h\")\n    replace_in_file_once(${SDIR}\/Common\/gdcmOpenSSLP7CryptographicMessageSyntax.cxx \"cert_ctx\" \"*cert_ctx\")\n    \n    replace_in_file_once(${SDIR}\/MediaStorageAndFileFormat\/gdcmJPEGLSCodec.cxx \"#include \\\"gdcm_charls.h\\\"\"\n        \"#include <header.h>\\n#include <interface.h>\\n#include <util.h>\\n#include <defaulttraits.h>\\n#include <losslesstraits.h>\\n#include <colortransform.h>\\n#include <processline.h>\")\n    \n    \n    # Define the srcs for Data Structure and Encoding Definitions\n    # DSED\n    \n    # dual compilation (namespace gdcm_ns), all the following either directly\n    # reference code using GDCM_SUPPORT_BROKEN_IMPLEMENTATION or indirectly\n    set(DSED2_SRCS\n      gdcmByteValue.cxx\n      gdcmDataElement.cxx\n      gdcmDataSet.cxx\n      gdcmExplicitDataElement.cxx\n      gdcmFile.cxx # FileMeta is class member\n      gdcmFileMetaInformation.cxx # subclass of DataSet\n      gdcmFragment.cxx\n      gdcmImplicitDataElement.cxx\n      gdcmItem.cxx\n      gdcmMediaStorage.cxx # SetFromModality takes a DataSet\n      gdcmPrivateTag.cxx\n      gdcmReader.cxx\n      gdcmSequenceOfFragments.cxx\n      gdcmSequenceOfItems.cxx\n      gdcmValue.cxx # friend decl\n      )\n    # need to prepare duplicate files to help cmake handle setting compile\n    # definitions (cmake cannot handle duplicate source file in same target).\n    # this trick allows us to avoid a static compilation which may not be portable\n    foreach(src ${DSED2_SRCS})\n      configure_file(${SDIR}\/DataStructureAndEncodingDefinition\/${src} ${BDIR}\/strict_${src} COPYONLY)\n      list(APPEND DSED3_SRCS ${BDIR}\/strict_${src})\n      set_property(\n        SOURCE ${BDIR}\/strict_${src}\n        PROPERTY COMPILE_DEFINITIONS \"GDCM_OVERRIDE_BROKEN_IMPLEMENTATION\" \"gdcm_ns=gdcmstrict\"\n        )\n    endforeach()\n    \n    # the following source code do not need a duplicate compilation to handle\n    # GDCM_SUPPORT_BROKEN_IMPLEMENTATION\n    set(src ${src} ${DSED3_SRCS})\n\ncheck_function_exists:\n    -   strcasecmp\n    -   strncasecmp\n    -   strptime\n    -   _snprintf\n    -   _stricmp\n    -   _strnicmp\n\ncheck_include_exists:\n    -   file: byteswap.h\n        variable: HAVE_BYTESWAP_H\n        cpp: false\n    -   file: langinfo.h\n        variable: HAVE_LANGINFO_H\n        cpp: false\n    -   file: rpc.h\n        variable: HAVE_RPC_H\n        cpp: false\n    -   file: sys\/time.h\n        variable: HAVE_SYS_TIME_H\n        cpp: false\n    -   file: winsock.h\n        variable: HAVE_WINSOCK_H\n        cpp: false\n\ncheck_symbol_exists:\n    -   symbol: snprintf\n        headers:\n            -   stdio.h\n\n",
